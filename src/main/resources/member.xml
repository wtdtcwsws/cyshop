<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyxz.cyshop.dao.MemberMapper">
<select id="getMember" parameterType="Integer" resultType="com.cyxz.cyshop.domain.Member">
        select  `id`,`name`,`phone` from member where `id` = #{id}
    </select>
    <select id="getMemberId" parameterType="Integer" resultType="String">
        select  `phone` from member where `id` = #{id}
    </select>


</mapper>