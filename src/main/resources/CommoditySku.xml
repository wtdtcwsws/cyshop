<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cyxz.cyshop.dao.CommoditySkuMapper">
    <select id="findBySkuId" parameterType="String" resultType="com.cyxz.cyshop.domain.Sku">
        select `description`,`price` from `commodity_sku` where `id` =
        #{sku_id}
    </select>
</mapper>